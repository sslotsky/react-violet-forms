{
  "name": "react-violet-forms",
  "version": "0.3.4",
  "description": "React form components built for use with the violet css framework. Works well with redux-form.",
  "main": "lib/index.js",
  "scripts": {
    "spec": "mocha './spec/**/*.spec.js*' --compilers js:babel-register --recursive",
    "spec-coverage": "nyc --require babel-core/register mocha --recursive './spec/**/*.spec.js*'",
    "lint": "eslint ./src/** ./spec/**/* --ext=js,jsx",
    "test": "npm run lint && npm run spec-coverage",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sslotsky/react-violet-forms.git"
  },
  "keywords": [
    "react",
    "forms",
    "violet"
  ],
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "exclude": [
      "examples",
      "build"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "author": "Sam Slotsky",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "expect": "^1.20.2",
    "istanbul": "^0.4.5",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "webpack": "^1.14.0"
  }
}
